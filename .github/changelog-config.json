{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 Bug Fixes",
      "labels": ["fix", "bug"]
    },
    {
      "title": "## 📦 Dependencies",
      "labels": ["dependencies"]
    },
    {
      "title": "## 📄 Documentation",
      "labels": ["documentation"]
    },
    {
      "title": "## 🔧 Maintenance",
      "labels": ["chore", "refactor"]
    },
    {
      "title": "## 🔄 Other Changes",
      "labels": []
    }
  ],
  "template": "${{CHANGELOG}}\n\n## 👥 Contributors\n\n${{CONTRIBUTORS}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}}) by @${{AUTHOR}}",
  "empty_template": "No changes in this release",
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "base_branches": [
    "master"
  ],
  "transformers": [],
  "ignore_labels": [
    "ignore"
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": [],
  "tag_resolver": {
    "method": "semver"
  },
  "pull_request_resolver": {
    "method": "merge_commits",
    "pattern": "^Merge pull request #(\\d+) from (.*)$",
    "on_error_pr_pattern": "(?<=\\()[0-9]+(?=\\))"
  },
  "include_unlabeled_prs": true,
  "duplicate_filter": {
    "pattern": ".*",
    "on_property": "title",
    "method": "equals"
  }
}